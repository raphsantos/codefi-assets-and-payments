{
  "name": "@consensys/contracts",
  "version": "6.9.1",
  "description": "Package used to compile Custom Smart Contracts into bytecode",
  "main": "src",
  "files": [
    "contracts",
    "tests",
    "src",
    "build"
  ],
  "scripts": {
    "clean": "rimraf ./dist ./build",
    "compile": "truffle compile && truffle compile --config ./truffle-config.js",
    "compile:aztec": "truffle compile && truffle compile --config ./truffle-config-aztec.js",
    "compile:tokens": "truffle compile && truffle compile --config ./truffle-config-tokens.js",
    "console": "truffle console",
    "consume": "ts-node src/consume",
    "release": "standard-version",
    "build": "npm run clean && npm run compile && npm run compile:aztec && npm run compile:tokens",
    "deploy": "ts-node src/deploy-contract",
    "generate-account": "orchestrate accounts generate --endpoint localhost:9092 --chain rinkeby",
    "get-catalog": "orchestrate contracts catalog --endpoint localhost:8080",
    "get-contract": "orchestrate contracts contract -e localhost:8080 -n Counter",
    "lint:all": "npm run lint && npm run lint:sol",
    "lint:sol": "solium -d contracts/ --fix",
    "lint": "eslint '{src,test}/**/*.ts' --fix",
    "orchestrate": "orchestrate",
    "prettier:check": "prettier -c '{src,test}/**/*.ts'",
    "register-chain": "ts-node src/register-chain",
    "register-contract": "orchestrate contracts register -e localhost:8080 -n Counter -f build/contracts/Counter.json",
    "register:all": "ts-node src/registerContracts",
    "remix": "python -mwebbrowser http://localhost:8080 && ./node_modules/remix-ide/bin/remix-ide",
    "send-tx": "ts-node src/send-tx",
    "test:cov": "npm run test:cov:codefi && npm run test:cov:aztec",
    "test:cov:aztec": "truffle --config ./truffle-config-aztec.js run coverage --network coverage --file='./test/contracts_aztec/*.test.ts' --temp aztec_build",
    "test:cov:codefi": "truffle --config ./truffle-config.js run coverage --network coverage --file='./test/codefi_contracts/*.test.ts' --temp codefi_build",
    "test:integration": "truffle test",
    "truffle": "truffle",
    "version": "scripts/version.js"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/ConsenSys/mesh/boilerplates/solidity/boilerplate-solidity.git"
  },
  "keywords": [
    "solidity",
    "ethereum",
    "smart",
    "contracts",
    "security"
  ],
  "license": "Apache",
  "bugs": {
    "url": "https://gitlab.com/ConsenSys/mesh/boilerplates/solidity/boilerplate-solidity/issues"
  },
  "homepage": "https://gitlab.com/ConsenSys/mesh/boilerplates/solidity/boilerplate-solidity",
  "devDependencies": {
    "@openzeppelin/contracts": "~4.7.3",
    "@openzeppelin/test-helpers": "^0.5.10",
    "@truffle/hdwallet-provider": "^1.2.1",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "ERC1400": "github:ConsenSys/UniversalToken#f1d5bcf6f9a6decf2cc2ec9c6c5cfbdcaf600661",
    "axios": "^0.21.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-register": "~6.23.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.0.0",
    "chai-bignumber": "^2.0.0",
    "dotenv": "~8.2.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-mocha": "^6.3.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "eth-gas-reporter": "^0.2.17",
    "ethereumjs-util": "^5.1.2",
    "ethers": "~4.0.45",
    "ganache-cli": "^6.9.1",
    "lint-staged": "~9.5.0",
    "pegasys-orchestrate": "~2.0.0-rc10",
    "prettier": "^2.0.5",
    "remix-ide": "^0.10.3",
    "rimraf": "^3.0.2",
    "solidity-coverage": "^0.7.14",
    "solium": "^1.2.5",
    "solium-plugin-zeppelin": "^0.0.2",
    "standard-version": "^8.0.0",
    "truffle": "^5.1.62",
    "ts-node": "^8.6.2",
    "typescript": "^4.3.5",
    "web3-utils": "^1.2.7"
  },
  "dependencies": {
    "@aztec/protocol": "^1.7.1"
  }
}
